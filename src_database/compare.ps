set terminal postscript eps enhanced color
set out 'whole_range.eps'
set encoding iso_8859_1

set multiplot layout 2,1

set title 'PES vs ab initio'
set xlabel 'r_5 ({\305})'
set ylabel 'Energy (cm^{-1})'

plot 'cut.plot' with lines lt rgb "black" lw 3 title 'PES','cut.plot' using 1:3 with lines lt rgb "violet" lw 3 title 'CCSD(T)-F12/aVTZ'

set title 'Differnece between PES and ab initio'
set xlabel 'r_5 ({\305})'
set ylabel 'Energy (cm^{-1})'
plot 'cut.plot' using 1:($2-$3) with lines lt rgb "black" lw 3 title 'PES-ab initio'
